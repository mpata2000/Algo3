TestCase subclass: #PruebasTresEnLinea	instanceVariableNames: ''	classVariableNames: ''	package: 'Parcial-Test'!!PruebasTresEnLinea methodsFor: 'tests' stamp: 'MartinPata 6/23/2021 16:14'!test10GanaCirculoConTresEnLinea	| juego |		juego := TaTeTi new.	juego cruzColumna: 1 Fila: 2.	juego circuloColumna: 1 Fila: 1.	juego cruzColumna: 2 Fila: 2.	juego circuloColumna: 2 Fila: 1.	juego cruzColumna: 3 Fila: 3.	juego circuloColumna: 3 Fila: 1.		self assert: (juego ganador) equals: 'Circulo'! !!PruebasTresEnLinea methodsFor: 'tests' stamp: 'MartinPata 6/23/2021 16:38'!test15GanaCruzConFila	| juego |		juego := TaTeTi new.	juego cruzColumna: 1 Fila: 3.	juego circuloColumna: 1 Fila: 2.	juego cruzColumna: 2 Fila: 3.	juego circuloColumna: 2 Fila: 2.	juego cruzColumna: 3 Fila: 3.		self assert: (juego ganador) equals: 'Cruz'! !!PruebasTresEnLinea methodsFor: 'tests' stamp: 'MartinPata 6/23/2021 16:32'!test13GanaCruzConDiagonalArribaAbjo	| juego |		juego := TaTeTi new.	juego cruzColumna: 1 Fila: 1.	juego circuloColumna: 1 Fila: 2.	juego cruzColumna: 2 Fila: 2.	juego circuloColumna: 1 Fila: 3.	juego cruzColumna: 3 Fila: 3.		self assert: (juego ganador) equals: 'Cruz'! !!PruebasTresEnLinea methodsFor: 'tests' stamp: 'MartinPata 6/23/2021 16:12'!test09NoSePuedeColocarCruzFueraDeRangoFila	| juego |		juego := TaTeTi new.		self should: [juego cruzColumna: 1 Fila: 4.] raise: PosicionFueraDeRango.! !!PruebasTresEnLinea methodsFor: 'tests' stamp: 'MartinPata 6/23/2021 15:24'!test01GanaCruzConTresEnLinea	| juego |		juego := TaTeTi new.	juego cruzColumna: 1 Fila: 1.	juego circuloColumna: 1 Fila: 2.	juego cruzColumna: 2 Fila: 1.	juego circuloColumna: 3 Fila: 3.	juego cruzColumna: 3 Fila: 1.		self assert: (juego ganador) equals: 'Cruz'! !!PruebasTresEnLinea methodsFor: 'tests' stamp: 'MartinPata 6/23/2021 16:31'!test12NoGanoCruzCirculoConTresEnLinea	| juego |		juego := TaTeTi new.	juego cruzColumna: 1 Fila: 2.	juego circulo